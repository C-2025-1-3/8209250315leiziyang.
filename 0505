#include<iostream>
using namespace std;
class point 
{
private:
	int x;
	int y;
public:
	point():x(60),y(80){}
	void setpoint(int i, int j)
	{
		x = 60+i;
		y = 80+j;
	}
	void display()
	{
		cout << "修改后的坐标为： " << x << y << endl;
	}
};
int main()
{
	point p;
	p.setpoint(10, 20);
	p.display();
	return 0;
}
